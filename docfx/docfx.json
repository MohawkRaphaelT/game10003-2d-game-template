{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "GAME 10003 Game Development Foundations - 2D Project Template/*.csproj"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filter.yml", 
      "_disableToc": true,
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Mohawk GAME 10003 Game Development Foundations - 2D Game Template",
      "_appTitle": "",
      "_enableSearch": true,
      "pdf": false
    }
  }
}